import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int numFunc, horasTrab;
		Double salario;
		
		numFunc = sc.nextInt();
		horasTrab = sc.nextInt();
		salario = sc.nextDouble();
		
		System.out.printf("", args)
	}

}
